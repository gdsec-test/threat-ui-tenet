    "react-dom": "^17.0.2",
    "react-highlight-within-textarea": "^2.1.2",
    "react-intl": "^4.5.3",
    "react-json-tree": "^0.13.0",
    "react-redux": "^7.1.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "size-limit": "^4.7.0",
    "url-loader": "^4.1.1",
    "url-parse": "^1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/register": "^7.0.0",
    "@godaddy/eslint-plugin-react-intl": "^1.1.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "chai": "^4.3.4",
    "enzyme": "^3.10.0",
    "eslint": "^8.6.0",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-config-godaddy-react": "^8.0.0",
    "eslint-config-next": "^12.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-next": "0.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jsdom": "^15.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "path-parse": "^1.0.7",
    "postcss": "^8.4.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.2",
    "postcss-rtlcss": "^3.5.2",
    "sass": "^1.29.0",
    "setup-env": "^1.2.2",
    "sinon": "^7.4.1",
    "stylelint": "^13.0.0",
    "stylelint-config-godaddy": "^0.4.0",
    "webpack": "^5.21.2"
  },
  "browserslist": [
    "extends browserslist-config-godaddy"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-godaddy"
    ]
  },
  "eslintIgnore": [
    "coverage/",
    "build/"
  ],
  "postcss": {
    "plugins": [
      "postcss-flexbugs-fixes",
      [
        "postcss-preset-env",
        {
          "autoprefixer": {
            "flexbox": "no-2009"
          },
          "stage": 3,
          "features": {
            "custom-properties": false
          }
        }
      ],
      "postcss-rtlcss"
    ]
  }
